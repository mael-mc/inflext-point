// Diagrama de Clases
digraph {
	rankdir=BT
	node [shape=record]
	App [label="{App|+ main(args: String[]): void}"]
	VistaPrincipalControlador [label="{VistaPrincipalControlador| - panelActual: Node\n - calcularControlador: CalcularControlador | + inicializar(): void}"]
	CalcularControlador [label="{CalcularControlador| - funcionDao: IFuncion\n - analizador: AnalizadorFuncion | + procesarFuncion(): void}"]
	Funcion [label="{Funcion| - expresion: String\n - tipo: TipoFuncion | + getExpresion(): String}"]
	Punto [label="{Punto| - x: double\n - y: double | + toString(): String}"]
	PuntoCritico [label="{PuntoCritico| - tipo: TipoPuntoCritico | + getTipo(): TipoPuntoCritico}"]
	Intervalo [label="{Intervalo| - inicio: double\n - fin: double | + getLongitud(): double}"]
	PuntoCritico -> Punto [arrowtail=onormal dir=back]
	AnalizadorFuncion [label="{AnalizadorFuncion| - derivador: DerivadorSimbolico\n - evaluador: Evaluador | + analizar(f: Funcion): ResultadoAnalisis}"]
	DerivadorSimbolico [label="{DerivadorSimbolico| | + derivar(exp: String): String}"]
	Evaluador [label="{Evaluador| | + evaluar(exp: String, x: double): double}"]
	ResultadoAnalisis [label="{ResultadoAnalisis| - primeraDerivada: String\n - segundaDerivada: String\n - puntosCriticos: List<PuntoCritico> | + getInforme(): String}"]
	IFuncion [label="{ \<\<interface\>\> \nIFuncion| + guardar(f: Funcion): void\n + listar(): List<Funcion>}"]
	FuncionImpl [label="{FuncionImpl| - historial: List<Funcion> | + guardar(f: Funcion): void}"]
	FuncionImpl -> IFuncion [label="\<\<implements\>\>" arrowtail=empty dir=back style=dashed]
	GraficadorCanvas [label="{GraficadorCanvas| - canvas: Canvas | + graficar(f: String): void}"]
	ValidadorExpresion [label="{ValidadorExpresion| | + esValida(exp: String): boolean}"]
	App -> VistaPrincipalControlador [label=inicia]
	VistaPrincipalControlador -> CalcularControlador [label=contiene]
	CalcularControlador -> AnalizadorFuncion [label=usa]
	CalcularControlador -> IFuncion [label=usa]
	CalcularControlador -> GraficadorCanvas [label=usa]
	CalcularControlador -> ValidadorExpresion [label=usa]
	AnalizadorFuncion -> DerivadorSimbolico [label=usa]
	AnalizadorFuncion -> Evaluador [label=usa]
	AnalizadorFuncion -> ResultadoAnalisis [label=produce]
}
