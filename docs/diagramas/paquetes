// Diagrama de Paquetes
digraph {
	rankdir=TB
	node [shape=tab]
	subgraph cluster_com {
		label="com.espoch.inflexpoint"
		color=ivory style=filled
		App [label=app]
		subgraph cluster_controladores {
			color=lightblue label=controladores
			Paneles [label=paneles]
			Vista [label=vistaprincipal]
			Vista -> Paneles [style=dashed]
		}
		subgraph cluster_modelos {
			color=lightgreen label=modelos
			Calculos [label=calculos]
			DAO [label=dao]
			Entidades [label=entidades]
			Enumeraciones [label=enumeraciones]
			Excepciones [label=excepciones]
			Calculos -> Entidades [style=dashed]
			Calculos -> Enumeraciones [style=dashed]
			DAO -> Entidades [style=dashed]
		}
		subgraph cluster_util {
			color=lightyellow label=util
			Util [label=util]
		}
		App -> Vista [label=inicia]
		Paneles -> Calculos [label=usa]
		Paneles -> DAO [label=usa]
		Paneles -> Util [label=usa]
		Calculos -> Excepciones [label=lanza]
	}
}
